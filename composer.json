{
    "name": "tafoyaventures/revive-xmlrpc",
    "description": "package of the library classes to provide XmlRpc routines for accessing revive ad servers",
    "homepage": "https://github.com/tafoyaventures/Revive-XmlRpc",
    "type": "library",
    "require": {
        "php" : ">=7.1",
        "phpxmlrpc/phpxmlrpc": "4.3.1",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "victorjonsson/markdowndocs": "1.3.*"
    },
    "keywords": [
        "revive",
        "adsserver",
        "api",
        "xmlrpc"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Charles Peterson",
            "email": "artistan@gmail.com"
        },
        {
            "name": "Brian Tafoya",
            "email": "btafoya@tafoyaventures.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Artistan\\ReviveXmlRpc\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Artistan\\ReviveXmlRpc\\Provider"
            ]
        }
    }
}
